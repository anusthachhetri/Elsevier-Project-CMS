{
  "$schema": "http://json-schema.org/schema#",
  "title": "Definitions for CUReport properties",
  "description": "This schema contains the generic definitions for the CU report as used in the Apollo Manifest. Do not use standalone",
  "definitions": {
    "typeIsCU": {
      "type": "string",
      "pattern": "^pf:CUReport$"
    },
    "contextIsCU": {
      "type": "object",
      "items": {"properties": {"cu": {
        "type": "string",
        "pattern": "^http://vtw.elsevier.com/data/ns/properties/Manifest/PreFlight-1/$"
      }}}
    },
    "cu:title": {
      "title": "title",
      "type": "boolean"
    },
    "cu:author_group": {
      "title": "author group",
      "type": "boolean"
    },
    "name": {
      "title": "figure name",
      "type": "string"
    },
    "id": {
      "title": "id",
      "type": "string"
    },
    "cu:affiliations": {
      "title": "affiliations",
      "type": "boolean"
    },
    "cu:corresponding_author_address": {
      "title": "corresponding author address",
      "type": "boolean"
    },
    "cu:corresponding_author_email_address": {
      "title": "corresponding author email address",
      "type": "boolean"
    },
    "cu:abstract": {
      "title": "abstract",
      "type": "boolean"
    },
    "cu:keywords": {
      "title": "keywords",
      "type": "boolean"
    },
    "cu:line_numbering": {
      "title": "line numbering",
      "type": "boolean"
