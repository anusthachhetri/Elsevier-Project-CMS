    },
    "cu:editable_table_count": {
      "title": "editable table count",
      "type": "integer"
    },
    "cu:missing_figures": {
      "title": "missing figures list",
      "description": "array of missing_figures",
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "id": {"$ref": "#/definitions/id"},
          "name": {"$ref": "#/definitions/name"}
